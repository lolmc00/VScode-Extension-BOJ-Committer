{
	"name": "boj-helper",
	"displayName": "BOJ Helper",
	"description": "",
	"version": "0.0.2",
	"publisher": "lolmc00",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.BOJTest",
		"onCommand:extension.BOJCommit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.BOJTest",
				"title": "[BOJ Committer] Run Testcase(s)"
			},
			{
				"command": "extension.BOJCommit",
				"title": "[BOJ Committer] Commit My source"
			}
		],
		"keybindings": [
			{
				"command": "extension.BOJCommit",
				"key": "ctrl+alt+G",
				"mac": "shift+cmd+G"
			},
			{
				"command": "extension.BOJTest",
				"key": "ctrl+alt+T",
				"mac": "shift+cmd+T"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.17",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/request": "^2.48.4",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"extensionDependencies": [
		"vscode.git"
	],
	"dependencies": {
		"@types/got": "^9.6.11",
		"@types/tmp": "^0.2.0",
		"cheerio": "^1.0.0-rc.3",
		"fs": "0.0.1-security",
		"got": "^11.5.1",
		"loader": "^2.1.1",
		"request": "^2.88.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/lolmc00/VScode-Extension-BOJ-Committer"
	}
}
